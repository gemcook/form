{
  "compilerOptions": {
    "target": "es2015",
    "module": "es6"
  },
  "path-intellisense.mappings": {
    "/": "${workspaceRoot}",
    "src": "${workspaceRoot}/src",
    "stories": "${workspaceRoot}/stories"
  },
  "typeAcquisition": {
    "include": [
      "prop-types",
      "ramda",
      "react",
      "react-dom",
      "react-redux",
      "recompose",
      "redux",
      "redux-form",
      "semantic-ui-react"
    ]
  }
}
